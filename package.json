{
  "name": "@maersk-global/vite-maersk-template",
  "version": "0.0.2594",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "vite",
    "start:e2e": "vite --port 5050",
    "build": "vite build",
    "preview": "vite build && vite preview --host 127.0.0.1 --port 5050",
    "preview:e2e": "vite build --mode e2e && vite preview --host 127.0.0.1 --port 5050",
    "test": "vitest run",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview:e2e http://127.0.0.1:5050/ 'cypress open --project tests/e2e'",
    "test:e2e:ci": "start-server-and-test preview:e2e http://127.0.0.1:5050/ 'cypress run --project tests/e2e'",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "copy:husky": "cp -r .husky/* .git/hooks/ ",
    "prepare": "husky install",
    "update-dependencies": "ncu -u"
  },
  "dependencies": {
    "@maersk-global/mds-components-core": "^2.2.3",
    "@maersk-global/shared-js": "^5.7.0",
    "@maersk-global/vue-plugin-maerskvueapp": "^1.4.0",
    "@vee-validate/rules": "^4.9.5",
    "pinia": "^2.1.3",
    "vee-validate": "^4.9.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@maersk-global/vite-plugin-datadog": "^1.2.0",
    "@maersk-global/vite-plugin-google-analytics": "^1.2.0",
    "@maersk-global/vite-plugin-mds-setup": "^1.0.3",
    "@pinia/testing": "^0.1.2",
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.13.0",
    "cypress": "^12.12.0",
    "eslint": "^8.41.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.14.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "nock": "^13.3.1",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.8",
    "vite-plugin-mock-server": "^1.1.0",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.2.0",
    "wait-for-expect": "^3.0.2"
  }
}
